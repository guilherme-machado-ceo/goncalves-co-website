{
  "routes": [
    {
      "route": "/",
      "serve": "/index.html"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "mimeTypes": {
    ".html": "text/html",
    ".css": "text/css",
    ".js": "application/javascript"
  }
}