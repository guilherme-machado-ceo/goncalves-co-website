{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/home",
      "rewrite": "/homepage.html"
    },
    {
      "route": "/sobre-nos",
      "rewrite": "/sobrenos.html"
    },
    {
      "route": "/servicos",
      "rewrite": "/nossosservicos.html"
    },
    {
      "route": "/contato",
      "rewrite": "/contato.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/homepage.html",
    "exclude": ["/imagens/*", "/*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/homepage.html",
      "statusCode": 302
    },
    "403": {
      "redirect": "/homepage.html",
      "statusCode": 302
    },
    "404": {
      "redirect": "/homepage.html",
      "statusCode": 302
    }
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".css": "text/css",
    ".js": "application/javascript",
    ".html": "text/html",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".gif": "image/gif",
    ".svg": "image/svg+xml",
    ".ico": "image/x-icon"
  }
}